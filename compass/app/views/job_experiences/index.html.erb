<p id="notice"><%= notice %></p>

<h1>Job Experiences</h1>

<table>
  <thead>
    <tr>
      <th>Job</th>
      <th>User</th>
      <th>Company</th>
      <th>Start of date</th>
      <th>End of date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @job_experiences.each do |job_experience| %>
      <tr>
        <td><%= job_experience.job_id %></td>
        <td><%= job_experience.user_id %></td>
        <td><%= job_experience.company_id %></td>
        <td><%= job_experience.start_of_date %></td>
        <td><%= job_experience.end_of_date %></td>
        <td><%= link_to 'Show', job_experience %></td>
        <td><%= link_to 'Edit', edit_job_experience_path(job_experience) %></td>
        <td><%= link_to 'Destroy', job_experience, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Job Experience', new_job_experience_path %>
